    "eslint": "^7.29.0",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^27.0.5",
    "jest-extended": "^0.11.5",
    "node-sass": "^6.0.1",
    "sass-loader": "^12.1.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.0.0",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2"
