    "eslint": "^7.24.0",
    "eslint-plugin-react": "^7.23.2",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "node-sass": "^5.0.0",
    "sass-loader": "^11.0.1",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0"
